-- Teleports Page
local TeleportTab = Window:CreateTab({
    Name = "TPs"
    })
    
local TeleportSection1 = TeleportTab:CreateSection({
    Name = "Main Locations"
})

local TeleportSection2 = TeleportTab:CreateSection({
    Name = "Sniping Areas"
})

local TeleportSection3 = TeleportTab:CreateSection({
    Name = "AFK Areas"
})

TeleportSection1:AddButton({
    Name = "Spawn",
    Callback = function()
        local plr = game:service"Players".LocalPlayer;
        local tween_s = game:service"TweenService";
        local info = TweenInfo.new(0,Enum.EasingStyle.Quad); -- 1 is the speed of it, set to 0 for the fastest
        function tp(...)
        local tic_k = tick();
        local params = {...};
        local cframe = CFrame.new(params[1],params[2],params[3]);
        local tween,err = pcall(function()
            local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
            tween:Play();
        end)
        if not tween then return err end
        end
        tp(-529, 23, 641);
    end
})

