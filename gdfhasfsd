local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/FateDecides/UILoader/main/UILoader"))()
local Wait = library.subs.Wait -- Only returns if the GUI has not been terminated. For 'while Wait() do' loops

local Window = library:CreateWindow({
Name = "Fate Hub",
Themeable = {}
})


-- Main Page
local MainTab = Window:CreateTab({
Name = "Weapons"
})

local GunSection1 = MainTab:CreateSection({
Name = "AutoBuy Ammo"
})

local GunSection2 = MainTab:CreateSection({
Name = "Instant Equip Guns"
})


GunSection1:AddButton({
    Name = "Buy Ammo Box",
    Callback = function()
        saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local plr = game:service"Players".LocalPlayer;
        local tween_s = game:service"TweenService";
        local info = TweenInfo.new(0,Enum.EasingStyle.Quad); -- 1 is the speed of it, set to 0 for the fastest
        function tp(...)
           local tic_k = tick();
           local params = {...};
           local cframe = CFrame.new(params[1],params[2],params[3]);
           local tween,err = pcall(function()
               local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
               tween:Play();
           end)
           if not tween then return err end
        end
        tp(-1225, 36, -186);
        wait(.2)
        local args = {
            [1] = "Ammo Box"
        }
        
        game:GetService("ReplicatedStorage").FE.BuyGear:InvokeServer(unpack(args))
        
        wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
    end
})

GunSection1:AddButton({
    Name = "Refill Berreta M9 Ammo",
    Callback = function()
        saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local plr = game:service"Players".LocalPlayer;
        local tween_s = game:service"TweenService";
        local info = TweenInfo.new(0,Enum.EasingStyle.Quad); -- 1 is the speed of it, set to 0 for the fastest
        function tp(...)
           local tic_k = tick();
           local params = {...};
           local cframe = CFrame.new(params[1],params[2],params[3]);
           local tween,err = pcall(function()
               local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
               tween:Play();
           end)
           if not tween then return err end
        end
        tp(-1225, 36, -186);
        wait(.2)
        for i = 1,6 do
            local args = {
            [1] = "Berreta M9"
        }
        
        game:GetService("ReplicatedStorage").FE.BuyAmmo:InvokeServer(unpack(args))
        end
        wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
    end
})

