    Name = "AutoBuy Tools"
})

AutoBuySection1:AddButton({
    Name = "Buy Beretta M9 ($900)",
    Callback = function()
        saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local plr = game:service"Players".LocalPlayer;
        local tween_s = game:service"TweenService";
        local info = TweenInfo.new(0,Enum.EasingStyle.Quad); -- 1 is the speed of it, set to 0 for the fastest
        function tp(...)
           local tic_k = tick();
           local params = {...};
           local cframe = CFrame.new(params[1],params[2],params[3]);
           local tween,err = pcall(function()
               local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
               tween:Play();
           end)
           if not tween then return err end
        end
        tp(-1225, 36, -186);
        wait(.2)
        local args = {
            [1] = 5743922
        }
        
        game:GetService("ReplicatedStorage").FE.PlayerOwnsPass:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "Beretta M9"
        }
        
        game:GetService("ReplicatedStorage").FE.BuyGun:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "Beretta M9",
            [2] = true
        }
        
        game:GetService("ReplicatedStorage").FE.EquipGun:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = game:GetService("Players").LocalPlayer
        }
        
        game:GetService("ReplicatedStorage").FE.GetGunData:InvokeServer(unpack(args))
        
        wait()
        for i = 1,4 do
        local args = {
            [1] = 5743922
        }
        
        game:GetService("ReplicatedStorage").FE.PlayerOwnsPass:InvokeServer(unpack(args))
        end
        wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
    end
})

