})

local berettass = false
local berettss = SoundsSection1:AddToggle({
    Name = "Spam Beretta M9",
    Callback = function(Berettass)
    berettass1 = Berettass
end})

local Loop = game:GetService("RunService").RenderStepped:Connect(function()
    if berettass1 then
        local args = {
            [1] = "Beretta M9",
            [2] = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Beretta M9").FrontEnd.Muzzle,
            [3] = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Beretta M9").Handle,
            [4] = true
        }

        game:GetService("ReplicatedStorage").Guns.Remotes.GunParticles:FireServer(unpack(args))
    end
end)

